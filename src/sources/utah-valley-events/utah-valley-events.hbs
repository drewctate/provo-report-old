<h3>Utah Valley Events Today</h3>
<table width="500" border="0" cellspacing="0" cellpadding="0">
    {{#each events}}
    <tr>
        <td>
            <img src="{{this.thumbnail}}" width="100" alt="{{this.title}}">
        </td>
        <td>
            <a target="_blank" href="{{this.url}}">{{this.title}}</a>
            <p>{{this.location}}</p>
        </td>
    </tr>
    {{/each}}
</table>